php_value display_errors 0

ErrorDocument 400 /apache-errors/400.html
ErrorDocument 401 /apache-errors/401.html
ErrorDocument 403 /apache-errors/403.html
ErrorDocument 404 /apache-errors/404.html
ErrorDocument 500 /apache-errors/500.html

RewriteEngine On
RewriteCond %{DOCUMENT_ROOT}%{REQUEST_URI} !-f
RewriteCond %{DOCUMENT_ROOT}%{REQUEST_URI} !-d
RewriteRule ^(.*)$ %{DOCUMENT_ROOT}/link.php [L]

ExpiresActive On
ExpiresDefault "access 1 second"